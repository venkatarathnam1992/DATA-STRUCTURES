#include<stdio.h>
int main()
{
int key,flag=0,i,n,A[50],pos;
printf("enter n:");
scanf("%d",&n);
printf("enter the array element:");
for(i=0;i<n;i++)
scanf("%d",&A[i]);
printf("enter the key element:");
scanf("%d",&key);
for(i=0;i<n;i++){
if(A[i]==key)
flag=1;
pos=i;
break;
}
if(flag==1)
printf("the given key is at a pos=%d",pos);
else
printf("the given key is not found",pos);
return 0;
}
